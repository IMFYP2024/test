// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_URL")
}

// prisma/schema.prisma

// prisma/schema.prisma

model SchoolData {
  id                    Int       @id @default(autoincrement())
  schoolYear            Int
  schoolCode            String
  schoolName            String
  departmentCode        String
  departmentName        String
  dayOrNight            String
  level                 String?
  studentCount          Int
  teacherCount          Int
  lastYearGraduateCount Int
  city                  String
  system                String
}


